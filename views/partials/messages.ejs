<link href="/css/style.css" rel="stylesheet" type="text/css">

<% if(typeof errors!= 'undefined') { %>
  <%    errors.forEach(function(error){ %>
          <p class="partials-text"> <%= error.msg %></p>
  <%    })        %>
  <% } %>
  
  <% if(success_msg!= '') { %>
          <p class="success-text"><%=success_msg %> </p>
        <% } %>
  
  <% if(error_msg != '') { %>
        <p class="partials-text"><%= error_msg %></p> 
        <% } %> 
  <% if(error != '') { %>
         <p class="partials-text"><%= error %> </p>
        <% } %> 